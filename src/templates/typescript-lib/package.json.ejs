{
  "name": "<%= props.package_name %>",
  "version": "0.0.0",
  "description": "",
  "keywords": [],
  "repository": "https://github.com/Raiper34/<%= props.package_name %>",
  "homepage": "https://<%= props.package_name %>.netlify.app",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Filip Raiper34 Gulan",
  "license": "MIT",
  "scripts": {
    "build": "npx tsc && browserify dist/index.js -o dist/browser-bundle.js -s <%= props.packageName %> && uglifyjs dist/browser-bundle.js -o dist/browser-bundle.min.js",
    "start": "npm run build && node dist/main.js",
    "test": "vitest run --coverage",
    "test:dev": "vitest --ui --coverage",
    "test:ci": "npm run test && cat ./coverage/lcov.info | coveralls",
    "lint": "eslint ./src",
    "lint:fix": "npm run lint -- --fix",
    "docs": "typedoc src/index.ts",
    "deploy": "npm run docs && netlify deploy --dir=docs --prod",
    "release": "release-it",
    "changelog": "auto-changelog -p",
    "commit": "cz",
    "prepare": "husky"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
     "@types/eslint__js": "^8.42.3",
     "@vitest/coverage-v8": "^3.0.1",
     "@vitest/ui": "^3.0.1",
     "auto-changelog": "^2.5.0",
     "browserify": "^17.0.1",
     "commitizen": "^4.3.1",
     "coveralls": "^3.1.1",
     "cz-conventional-changelog": "^3.3.0",
     "eslint": "^9.13.0",
     "husky": "^9.1.7",
     "netlify-cli": "^17.37.1",
     "release-it": "^17.11.0",
     "typedoc": "^0.26.10",
     "typescript": "^5.6.0",
     "typescript-eslint": "^8.10.0",
     "uglify-js": "^3.19.3",
     "vitest": "^3.0.1"
  }
}