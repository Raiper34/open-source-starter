{
  "name": "<%= props.package_name %>",
  "version": "0.0.0",
  "description": "",
  "keywords": [],
  "repository": "https://github.com/Raiper34/<%= props.package_name %>",
  "homepage": "https://<%= props.package_name %>.netlify.app",
  "author": "Filip Raiper34 Gulan",
  "license": "MIT",
  "main": "./dist/<%= props.package_name %>.umd.cjs",
  "module": "./dist/<%= props.package_name %>.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/<%= props.package_name %>.js",
      "require": "./dist/<%= props.package_name %>.umd.cjs"
    }
  },
  "files": [
    "src",
    "dist",
    "test",
    "CHANGELOG.md",
    ".nvmrc"
  ],
  "scripts": {
    "build": "tsc && vite build",
    "start": "npm run build && node dist/main.js",
    "test": "vitest run --coverage",
    "test:dev": "vitest --ui --coverage",
    "test:ci": "npm run test && cat ./coverage/lcov.info | coveralls",
    "lint": "eslint ./src",
    "lint:fix": "npm run lint -- --fix",
    "docs": "typedoc src/index.ts",
    "deploy": "npm run docs && netlify deploy --dir=docs --prod",
    "release": "release-it",
    "changelog": "auto-changelog -p",
    "commit": "cz",
    "prepare": "husky"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@release-it/bumper": "^7.0.1",
    "@types/eslint__js": "^8.42.3",
    "@vitest/coverage-v8": "^3.0.1",
    "@vitest/ui": "^3.0.1",
    "auto-changelog": "^2.5.0",
    "commitizen": "^4.3.1",
    "coveralls": "^3.1.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.13.0",
    "husky": "^9.1.7",
    "netlify-cli": "^17.37.1",
    "release-it": "^18.1.2",
    "typedoc": "^0.27.6",
    "typedoc-material-theme": "^1.3.0",
    "typescript": "^5.6.0",
    "typescript-eslint": "^8.10.0",
    "vite": "^6.0.11",
    "vite-plugin-dts": "^4.5.0",
    "vitest": "^3.0.1"
  }
}